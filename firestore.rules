rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read;
	  allow create : if request.auth.uid != null;
	//   allow delete: if request.auth.uid != resource.data.author.id;
	//   Read, Create, Delete = Write
	//   allow write: if false;
    }
  }
}